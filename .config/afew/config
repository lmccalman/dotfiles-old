[global]

[MailMover]
folders = gmail/INBOX
gmail/INBOX = 'NOT tag:inbox':gmail/AllMail

[KillThreadsFilter]
[ListMailsFilter]
[Filter.1]
query = folder:gmail/Sent
tags = +sent;-inbox
[Filter.2]
query = folder:gmail/Drafts
tags = +draft
[Filter.3]
query = folder:gmail/AllMail
tags = -inbox
[InboxFilter]
